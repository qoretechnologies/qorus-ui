  "scripts": {
    "stop": "if [ ! -f \"$PIDFILE\" ]; then\n    echo Error: Expected \\$PIDFILE to be a file path, but it is \\\"$PIDFILE\\\".\n    exit 1\n  fi\n  kill -INT `cat \"$PIDFILE\"`",
    "build": "NODE_ENV=production webpack -p",
    "bgserver:start": "npm start &",
    "bgserver:stop": "npm stop",
    "test": "export npm_config_loglevel=silent NODE_ENV=test PIDFILE=.test.pid\n  npm run-script test:unit:all && \\\n    npm run-script bgserver:start && \\\n    npm run-script test:uat:all\n  test_status=$?\n  npm run-script bgserver:stop\n  exit $test_status",
    "test:ci": "export npm_config_loglevel=silent NODE_ENV=test PIDFILE=.test.pid\n  npm run-script test:unit:ci && \\\n    npm run-script bgserver:start && \\\n    npm run-script test:uat:ci\n  test_status=$?\n  npm run-script bgserver:stop\nexit $test_status",
    "test:uat": "NODE_ENV=test cucumber.js",
    "test:uat:all": "export npm_config_loglevel=silent\n  npm run-script test:uat -- -t ~@no-impl",
    "test:uat:feature": "export npm_config_loglevel=silent NODE_ENV=test PIDFILE=.test.pid\n  npm run-script bgserver:start && \\\n  npm run-script test:uat -- -t ~@no-impl\n  test_status=$?\n  npm run-script bgserver:stop\n  exit $test_status",
    "test:uat:ci": "export npm_config_loglevel=silent\n  npm run-script test:uat -- -p ci",
    "test:uat:wip": "export npm_config_loglevel=silent NODE_ENV=test PIDFILE=.wip.pid\n  npm run-script bgserver:start && \\\n    nodemon `which cucumber.js` -p wip",
    "test:unit": "NODE_ENV=test mocha",
    "test:unit:ci": "export npm_config_loglevel=silent\n  npm run-script test:unit -- -C 'test/**/*.spec.{js,jsx}'",
    "test:unit:all": "export npm_config_loglevel=silent\n  npm run-script test:unit -- 'test/**/*.spec.{js,jsx}'",
    "test:unit:wip": "export npm_config_loglevel=silent\n  npm run-script test:unit -- -R min -w 'test/**/*.spec.{js,jsx}'",
    "lint": "eslint --ext .js --ext .jsx api cucumber.js server.js src test types webpack.config webpack.config.js",
    "flow": "flow check"
  }
